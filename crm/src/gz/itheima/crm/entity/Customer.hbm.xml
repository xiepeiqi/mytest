<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="gz.itheima.crm.entity.Customer" table="cst_customer" catalog="itheimacrm">
        <id name="custId" type="java.lang.Long">
            <column name="cust_id" />
            <generator class="native"></generator>
        </id>
        <property name="custName" type="java.lang.String">
            <column name="cust_name" length="32" not-null="true">
                <comment>客户名称(公司名称)</comment>
            </column>
        </property>
        <property name="custSource" type="java.lang.String">
            <column name="cust_source" length="32">
                <comment>客户信息来源</comment>
            </column>
        </property>
        <property name="custIndustry" type="java.lang.String">
            <column name="cust_industry" length="32">
                <comment>客户所属行业</comment>
            </column>
        </property>
        <property name="custLevel" type="java.lang.String">
            <column name="cust_level" length="32">
                <comment>客户级别</comment>
            </column>
        </property>
        <property name="custPhone" type="java.lang.String">
            <column name="cust_phone" length="64">
                <comment>固定电话</comment>
            </column>
        </property>
        <property name="custMobile" type="java.lang.String">
            <column name="cust_mobile" length="16">
                <comment>移动电话</comment>
            </column>
        </property>
        <set name="linkmans" inverse="true">
            <key>
                <column name="lkm_cust_id" not-null="true">
                    <comment>客户id</comment>
                </column>
            </key>
            <one-to-many class="gz.itheima.crm.entity.Linkman" />
        </set>
    </class>
</hibernate-mapping>
